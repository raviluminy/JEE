<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>JEE-Projet</display-name>
	<servlet>
		<servlet-name>Controler</servlet-name>
		<servlet-class>fr.jee.projet.servlet.Controler</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>db_url</param-name>
		<param-value>jdbc:postgresql://localhost/jee</param-value>
	</context-param>
	<context-param>
		<param-name>db_user</param-name>
		<param-value>lionel</param-value>
	</context-param>
	<context-param>
		<param-name>db_pass</param-name>
		<param-value>projet</param-value>
	</context-param>
</web-app>